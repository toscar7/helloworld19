pipeline {
  agent any
  stages {
    stage('build'){
      steps {
       echo "build steps"
       sleep 10
      }
    }
     stage('test'){
      steps {
       echo "test steps"
       sleep 10
      }
    }
     stage('deploy'){
      steps {
       echo "deploy steps"
       sleep 10
      }
    }
     stage('docker'){
      steps {
       echo "image steps"
       sleep 10
      }
    }
   }
}
